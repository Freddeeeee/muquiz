@model string[]

@foreach (var item in Model)
{
    <button class="btn btn-alternatives btn-block btn-lg submit" value="@item">@item</button>
}

<br/> 

<div class="progress">
    <div class="progress-bar" aria-valuemin="0" aria-valuemax="100" aria-valuenow="0"></div>
</div>

<script type="text/javascript">
    var i = 0;

    function loadingTimeBar() {
        i += 1;
        if (i < 100) {
            i = i + 1;
            $(".progress-bar").css("width", i + "%");
        }
        setTimeout("loadingTimeBar()", 100);
    };

    loadingTimeBar();

    $(".submit").click(async function (event) {
        await window.submitAnswer(event.target.value);
    });

    $("body").removeClass().addClass("alternatives");
</script>